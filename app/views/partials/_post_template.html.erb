<section class="postTemplate hidden">
	<div class="newPost" id="{{id}}">
		<p>
			<label for="blog_title">Title</label>
		</p>
		<p class="title-area">
			<input type="text" name="blog_{{id}}[title]" id="blog_title">
		</p>
		<p>
			<label for="blog_post">Post</label>
		</p>
		<p class="post-area">
			<textarea name="blog_{{id}}[post]" id="blog_post"></textarea>
		</p>
	</div>
</section>
